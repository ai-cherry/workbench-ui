{
  "mcpServers": {
    "memory": {
      "command": "python",
      "args": [
        "../sophia-intel-ai/mcp/memory_server.py"
      ],
      "env": {
        "PYTHONPATH": "../sophia-intel-ai",
        "MCP_PORT": "8081",
        "MCP_HOST": "127.0.0.1",
        "LOG_LEVEL": "INFO"
      }
    },
    "filesystem": {
      "command": "python",
      "args": [
        "../sophia-intel-ai/mcp/filesystem/server.py"
      ],
      "env": {
        "PYTHONPATH": "../sophia-intel-ai",
        "MCP_PORT": "8082",
        "MCP_HOST": "127.0.0.1",
        "ALLOWED_DIRECTORIES": "../sophia-intel-ai,../workbench-ui",
        "LOG_LEVEL": "INFO"
      }
    },
    "analytics": {
      "command": "python",
      "args": [
        "../sophia-intel-ai/mcp/analytics/server.py"
      ],
      "env": {
        "PYTHONPATH": "../sophia-intel-ai",
        "MCP_PORT": "8083",
        "MCP_HOST": "127.0.0.1",
        "LOG_LEVEL": "INFO"
      }
    },
    "git": {
      "command": "python",
      "args": [
        "../sophia-intel-ai/mcp/git/server.py"
      ],
      "env": {
        "PYTHONPATH": "../sophia-intel-ai",
        "MCP_PORT": "8084",
        "MCP_HOST": "127.0.0.1",
        "GIT_REPO_PATH": "../workbench-ui",
        "LOG_LEVEL": "INFO"
      }
    },
    "unified": {
      "command": "python",
      "args": [
        "../sophia-intel-ai/mcp/unified/server.py"
      ],
      "env": {
        "PYTHONPATH": "../sophia-intel-ai",
        "MCP_PORT": "8085",
        "MCP_HOST": "127.0.0.1",
        "MEMORY_PORT": "8081",
        "FILESYSTEM_PORT": "8082",
        "ANALYTICS_PORT": "8083",
        "GIT_PORT": "8084",
        "LOG_LEVEL": "INFO"
      }
    }
  },
  "apiKeys": {
    "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
    "OPENAI_API_KEY": "${OPENAI_API_KEY}",
    "PORTKEY_API_KEY": "${PORTKEY_API_KEY}"
  },
  "modelPreferences": {
    "default": "claude-3-5-sonnet-20241022",
    "fallback": "gpt-4-turbo-preview",
    "fast": "claude-3-haiku-20240307"
  },
  "workspaceSettings": {
    "autoStartServers": true,
    "serverHealthCheckInterval": 30000,
    "maxRetries": 3,
    "retryDelay": 1000
  }
}