{
  "// EXTERNAL MCP SERVERS": "These servers are for viewing the sophia-intel-ai codebase, NOT for this workbench-ui project",
  "// DO NOT USE these servers for workbench-ui operations": "",
  
  "mcpServers": {
    "// EXTERNAL: Session memory and context management for sophia-intel-ai": "",
    "sophia-memory": {
      "type": "http",
      "url": "http://localhost:8081",
      "headers": {
        "Authorization": "Bearer dev-token",
        "Content-Type": "application/json"
      },
      "description": "EXTERNAL - Session memory and context management for sophia-intel-ai codebase",
      "capabilities": [
        "session.store",
        "session.retrieve",
        "memory.search",
        "context.maintain"
      ]
    },
    
    "// EXTERNAL: File operations and code intelligence for sophia-intel-ai": "",
    "sophia-filesystem": {
      "type": "http",
      "url": "http://localhost:8082",
      "headers": {
        "Authorization": "Bearer dev-token",
        "Content-Type": "application/json"
      },
      "description": "EXTERNAL - File operations and code intelligence for sophia-intel-ai codebase",
      "capabilities": [
        "file.read",
        "file.write",
        "file.list",
        "code.search",
        "symbols.index",
        "symbols.search"
      ]
    },
    
    "// EXTERNAL: Git operations and version control for sophia-intel-ai": "",
    "sophia-git": {
      "type": "http",
      "url": "http://localhost:8084",
      "headers": {
        "Authorization": "Bearer dev-token",
        "Content-Type": "application/json"
      },
      "description": "EXTERNAL - Git operations and version control for sophia-intel-ai codebase",
      "capabilities": [
        "git.status",
        "git.log",
        "git.branches",
        "git.diff"
      ]
    },
    
    "// EXTERNAL: Vector search and semantic indexing for sophia-intel-ai": "",
    "sophia-vector": {
      "type": "http",
      "url": "http://localhost:8085",
      "headers": {
        "Authorization": "Bearer dev-token",
        "Content-Type": "application/json"
      },
      "description": "EXTERNAL - Vector search and semantic indexing for sophia-intel-ai codebase",
      "capabilities": [
        "vector.index",
        "vector.search",
        "semantic.similarity",
        "embedding.generate"
      ]
    }
  },
  
  "// Configuration for external servers": "",
  "defaultTimeout": 30000,
  "retryAttempts": 3,
  "retryDelay": 1000,
  
  "// NOTE": "For internal MCP servers used by workbench-ui, see .roo/mcp.json"
}