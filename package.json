{
  "name": "workbench-ui",
  "version": "0.1.0",
  "description": "Sophia Intel AI Workbench Frontend UI with Agno AI v2",
  "author": "AI Cherry",
  "license": "MIT",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "mcp:connect": "node scripts/mcp-connector.js",
    "agno:init": "tsx agno/scripts/init.ts",
    "agno:dev": "tsx agno/scripts/dev.ts",
    "agno:agents": "tsx agno/scripts/agents.ts list",
    "agno:chain": "tsx agno/scripts/chain.ts run",
    "agno:ui": "tsx agno/scripts/ui.ts --mode hybrid",
    "agno:test": "jest --testPathPattern=agno --coverage",
    "agno:health": "tsx agno/scripts/health.ts"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@tanstack/react-query": "^5.28.0",
    "axios": "^1.6.8",
    "zustand": "^4.5.2",
    "zod": "^3.22.4",
    "@radix-ui/themes": "^3.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.2",
    "lucide-react": "^0.363.0",
    "react-hot-toast": "^2.4.1",
    "@agno-agi/core": "^2.0.0",
    "@agno-agi/ui": "^2.0.0",
    "@agno-agi/tools": "^2.0.0",
    "portkey-ai": "^1.3.0",
    "@langchain/core": "^0.2.0",
    "@langchain/community": "^0.2.0",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "js-yaml": "^4.1.0",
    "dotenv": "^16.4.5",
    "eventemitter3": "^5.0.1",
    "p-retry": "^6.2.0",
    "p-queue": "^8.0.1",
    "fastify": "^4.26.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/helmet": "^11.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/js-yaml": "^4.0.9",
    "typescript": "^5.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "prettier": "^3.2.5",
    "@testing-library/react": "^15.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.0",
    "nodemon": "^3.1.0"
  }
}